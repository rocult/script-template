{
  "luau-lsp.completion.autocompleteEnd": true,
  "luau-lsp.completion.imports.enabled": true,
  "luau-lsp.completion.imports.requireStyle": "alwaysRelative",
  "luau-lsp.completion.imports.separateGroupsWithLine": true,
  "luau-lsp.completion.imports.stringRequires.enabled": true,
  "luau-lsp.diagnostics.strictDatamodelTypes": true,
  "luau-lsp.diagnostics.workspace": true,
  "luau-lsp.hover.multilineFunctionDefinitions": true,
  "luau-lsp.hover.showTableKinds": true,
  "luau-lsp.platform.type": "roblox",
  "luau-lsp.sourcemap.enabled": true,
  "luau-lsp.sourcemap.sourcemapFile": "./.meta/sourcemap.json",
  "luau-lsp.sourcemap.generatorCommand": "pesde run sourcemap_generator > .meta/sourcemap.json",
  "luau-lsp.types.robloxSecurityLevel": "RobloxScriptSecurity",
  "luau-lsp.ignoreGlobs": ["/Packages/**", "/*_packages/**", "/dist.lua"],
  "luau-lsp.types.definitionFiles": [
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/types/environment.d.luau",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/types/luarmor.d.luau",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/types/luraph.d.luau"
  ],
  "luau-lsp.types.documentationFiles": [
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/docs/environment.d.json",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/docs/luarmor.d.json",
    "./luau_packages/.pesde/stefanuk12+luau_defs/0.2.0/luau_defs/docs/luraph.d.json"
  ],
  "selene.configPath": "./.meta/selene.toml",
  "stylua.configPath": "./.meta/.stylua.toml",
  "workbench.editor.customLabels.patterns": {
    "**/init.lua": "${dirname} (${filename}.${extname})",
    "**/init.luau": "${dirname} (${filename}.${extname})"
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": false
  },
  "[luau]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
    "editor.formatOnSave": true
  }
}
